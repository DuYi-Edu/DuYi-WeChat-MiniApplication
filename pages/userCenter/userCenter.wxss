.container{
  width: 100%;
  height: 100%;
  background-color: rgb(164, 188, 238);
}

.page-area{
  width: 90%;
  background-color: white;
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  padding: 30rpx 0;
}

.logoImg{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}

.page-section{
  display: flex;
  flex-direction: column;
  justify-content: flex-start
}

.username{
  font-size: 60rpx;
}